		  if (chmod(".", 0) < 0)
		    exit(3);
		  /* Test won't work if run as root - so don't be root */
		  if (getuid() == 0 || geteuid() == 0)
		    setresuid(1, 1, 1);	/* hpux has this */
#ifdef HAVE_GETWD /* silly since HAVE_* tests haven't been done yet */
		  {
		      extern char *getwd();
		      ok = getwd(buf) == 0;
		  }
#else
		  {
		      extern char *getcwd();
		      ok = getcwd(buf, sizeof(buf)) == 0;
		  }
#endif
		  exit(ok ? 0 : 10);
		  return ok ? 0 : 10;
		}
EOF
