+NS_InvokeByIndex_P:
+	stwu    sp,-32(sp)                      // setup standard stack frame
+	mflr    r0                              // save LR
+	stw     r3,8(sp)                        // r3 <= that
+	stw     r4,12(sp)                       // r4 <= methodIndex
 
