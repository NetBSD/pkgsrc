tolower needs unsigned char argument.

--- src/actioncompiler/compile.c.orig	2008-04-10 22:32:47.000000000 +0000
+++ src/actioncompiler/compile.c
@@ -595,7 +595,7 @@ void lower(char *s)
 {
 	while(*s)
 	{
-		*s = tolower(*s);
+		*s = tolower((unsigned char)*s);
 		++s;
 	}
 }
